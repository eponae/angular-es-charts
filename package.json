{
  "name": "conservatories",
  "version": "0.0.2",
  "devDependencies": {
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3",
    "extract-text-webpack-plugin": "3.0.1",
    "html-webpack-plugin": "2.30.1",
    "sasslint-webpack-plugin": "1.0.4",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-eslint": "8.0.1",
    "eslint": "4.9.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-config-airbnb-base": "12.1.0",
    "prettier-eslint-cli": "4.4.0",
    "html-loader": "0.5.1",
    "css-loader": "0.28.7",
    "style-loader": "0.19.0",
    "sass": "1.0.0-beta.2",
    "sass-lint": "1.12.1",
    "node-sass": "4.5.3",
    "sass-loader": "6.0.6",
    "file-loader": "1.1.5"
  },
  "description": "## Usage ### Requirements * [NodeJS](http://nodejs.org/) (with [NPM](https://www.npmjs.org/)) * [Webpack](https://webpack.github.io/docs/)",
  "main": "gulpfile.js",
  "scripts": {
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "clean": "rm -fr ./dist && mkdir ./dist",
    "copy-translations": "mkdir -p ./dist/locales && cp -a ./src/assets/locales/*.json ./dist/locales/",
    "copy-en-locale": "cp -a ./node_modules/angular-i18n/angular-locale_fr.js ./dist/locales/angular-i18n/",
    "copy-fr-locale": "cp -a ./node_modules/angular-i18n/angular-locale_en.js ./dist/locales/angular-i18n/",
    "copy-locales": "mkdir -p ./dist/locales/angular-i18n && npm run copy-fr-locale && npm run copy-en-locale",
    "copy-internationalization": "npm run copy-translations && npm run copy-locales",
    "copy-fonts": "mkdir -p ./dist/fonts && cp ./node_modules/material-design-icons/iconfont/MaterialIcons-Regular.* ./dist/fonts/",
    "copy-icon": "cp -a ./src/assets/favicon/. ./dist/",
    "copy-img": "cp -a ./src/assets/img/. ./dist/img/",
    "copy-assets": "npm run copy-internationalization && npm run copy-fonts && npm run copy-icon && npm run copy-img",
    "copy-templates": "mkdir -p ./dist/templates && cp ./src/scripts/**/**/marker-window.html ./dist/templates/marker-window.html",
    "copy": "npm run copy-templates && npm run copy-assets",
    "prebuild": "npm run clean && npm run copy && npm run format",
    "start": "npm run prebuild && GOOGLE_MAPS_KEY=AIzaSyDOaHhIXyhaoX9I692e6YUBVYCicLvED5A webpack-dev-server --port 4242 --hot --open --content-base dist/",
    "build": "PROD=true webpack -p"
  },
  "author": "Alice R",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/eponae/angular-es-charts.git"
  },
  "bugs": {
    "url": "https://github.com/eponae/angular-es-charts/issues"
  },
  "homepage": "https://github.com/eponae/angular-es-charts#readme",
  "dependencies": {
    "lodash": "4.17.4",
    "angular": "1.6.6",
    "angular-i18n": "1.6.6",
    "angular-ui-router": "0.4.3",
    "angular-google-maps": "2.4.1",
    "zingchart": "2.6.1",
    "zingchart-angularjs": "1.2.0",
    "angular-mocks": "1.6.6",
    "angular-simple-logger": "0.1.7",
    "angular-animate": "1.6.6",
    "angular-aria": "1.6.6",
    "angular-messages": "1.6.6",
    "angular-material": "1.1.5",
    "material-design-icons": "3.0.1",
    "angular-dynamic-locale": "0.1.32",
    "angular-translate": "2.16.0",
    "angular-translate-loader-static-files": "2.16.0",
    "angular-translate-storage-cookie": "2.16.0",
    "angular-translate-storage-local": "2.16.0",
    "angular-cookies": "1.6.6"
  }
}
